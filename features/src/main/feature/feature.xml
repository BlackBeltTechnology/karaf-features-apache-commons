<?xml version="1.0" encoding="UTF-8"?>
<features name="karaf-feature-apache-commons-${project.version}"
    xmlns="http://karaf.apache.org/xmlns/features/v1.5.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://karaf.apache.org/xmlns/features/v1.5.0 http://karaf.apache.org/xmlns/features/v1.5.0">

    
    <feature name="apache-commons" version="${project.version}" description="Apache Commons All" install="false">
<!--        <feature>javax-mail-api</feature>
        <feature>javax-servlet-api</feature>
        <feature dependency="true">transaction-api</feature> -->
        <feature>apache-commons-beanutils</feature>
        <feature>apache-commons-collections</feature>
        <feature>apache-commons-collections4</feature>
        <feature>apache-commons-io</feature>
        <feature>apache-commons-pool</feature>
        <feature>apache-commons-pool2</feature>
        <feature>apache-commons-dbcp2</feature>
        <feature>apache-commons-codec</feature>
        <feature>apache-commons-fileupload</feature>
        <feature>apache-commons-lang</feature>
        <feature>apache-commons-logging</feature>
        <feature>apache-commons-configuration</feature>
        <feature>apache-commons-math3</feature>
        <feature>apache-commons-math</feature>
        <feature>apache-commons-lang3</feature>
        <feature>apache-commons-csv</feature>
        <feature>apache-commons-compress</feature>
    </feature>

    <feature name="apache-commons-beanutils" version="1.9.2" description="Apache Commons Bean Utils" install="false">
        <feature>apache-commons-collections</feature>
        <bundle>mvn:commons-beanutils/commons-beanutils/1.9.2</bundle>
    </feature>

    <feature name="apache-commons-collections" version="3.2.2" description="Apache Commons Collections" install="false">
        <bundle>mvn:commons-collections/commons-collections/3.2.2</bundle>
    </feature>

    <feature name="apache-commons-collections4" version="4.3" description="Apache Commons Collections4" install="false">
        <bundle>mvn:org.apache.commons/commons-collections4/4.3</bundle>
    </feature>
    
    <feature name="apache-commons-io" version="2.6" description="Apache Commons IO" install="false">
        <bundle>mvn:commons-io/commons-io/2.6</bundle>
    </feature>
    
    <feature name="apache-commons-pool" version="1.6" description="Apache Commons Pool" install="false">
        <bundle>mvn:commons-pool/commons-pool/1.6</bundle>
    </feature>
    
    <feature name="apache-commons-pool2" version="2.4.1" description="Apache Commons Pool2" install="false">
        <bundle>mvn:org.apache.commons/commons-pool2/2.4.1</bundle>
    </feature>
    
    <feature name="apache-commons-dbcp2" version="2.1.1" description="Apache Commons DBCP2" install="false">
        <feature dependency="true">transaction-api</feature>
        <feature>apache-commons-pool2</feature>
        <bundle>mvn:org.apache.commons/commons-dbcp2/2.1.1</bundle>
    </feature>
    
    <feature name="apache-commons-codec" version="1.12" description="Apache Commons Codec" install="false">
        <bundle>mvn:commons-codec/commons-codec/1.12</bundle>
    </feature>
    
    <feature name="apache-commons-fileupload" version="1.3.1" description="Apache Commons Fileupload" install="false">
        <feature dependency="true">javax-servlet-api</feature>
        <feature>apache-commons-io</feature>
        <bundle>mvn:commons-fileupload/commons-fileupload/1.3.1</bundle>
    </feature>
    
    <feature name="apache-commons-lang" version="2.6" description="Apache Commons Lang" install="false">
        <bundle>mvn:commons-lang/commons-lang/2.6</bundle>
    </feature>
    
    <feature name="apache-commons-logging" version="1.1.1" description="Apache Commons Logging" install="false">
        <bundle>mvn:org.apache.commons/com.springsource.org.apache.commons.logging/1.1.1</bundle>
    </feature>
    
    <feature name="apache-commons-configuraton" version="1.10" description="Apache Commons Configuration" install="false">
        <bundle>mvn:commons-configuration/commons-configuration/1.10</bundle>
    </feature>
    
    <feature name="apache-commons-math3" version="3.6.1" description="Apache Commons Math3" install="false">
        <bundle>mvn:org.apache.commons/commons-math3/3.6.1</bundle>
    </feature>
    
    <feature name="apache-commons-math" version="2.2" description="Apache Commons Math" install="false">
        <bundle>mvn:org.apache.commons/commons-math/2.2</bundle>
    </feature>
    
    <feature name="apache-commons-lang2" version="3.6" description="Apache Commons Lang3" install="false">
        <bundle>mvn:org.apache.commons/commons-lang3/3.8</bundle>
    </feature>
    
    <feature name="apache-commons-csv" version="1.6" description="Apache Commons CSV" install="false">
        <bundle>mvn:org.apache.commons/commons-csv/1.6</bundle>
    </feature>
    
    <feature name="apache-commons-compress" version="1.18" description="Apache Commons Compress" install="false">
        <bundle>mvn:org.apache.commons/commons-compress/1.18</bundle>
    </feature>

</features>
